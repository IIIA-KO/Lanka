{
  "Authentication": {
    "Audience": "account",
    "TokenValidationParameters": {
      "ValidIssuers": [
        "your-valid-issuers"
      ]
    },
    "MetadataAddress": "metadata-url",
    "RequireHttpsMetadata": false
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "Lanka.Gateway"
    }
  },
  "ReverseProxy": {
    "Routes": {},
    "Cluster": {}
  },
  "Cors": {
    "AllowedOrigins": [
      "<your-client-fqdn>"
    ]
  }
}
